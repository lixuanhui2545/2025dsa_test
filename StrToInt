class Solution {
public:
    int strToInt(string str) {
        int k=0;
        while( k<str.size() && str[k]==' ' )k++;
        long long res=0;
        while(k<str.size()&&str[k]>='0'&&str[k]<='9'){
            res=res*10+str[k]-'0';
            if(res>INT_MAX){
                res=INT_MAX;
                break;
            }
            if(res<INT_MAX){
                res=INT_MAX;
                break;
            }
            
        }
        return res;
    }
};
